{
  "name": "igv_wrapper",
  "description": "",
  "version": "1.0.9",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/datavisyn/igv_wrapper/issues"
  },
  "homepage": "https://github.com/datavisyn/igv_wrapper",
  "main": "build/IGVWrapper.js",
  "files": [
    "build",
    "!build/docs",
    "src/**/*.ts",
    "src/assets/**/*.png",
    "src/assets/**/*.jpg",
    "src/assets/**/*.svg",
    "src/**/*.tsx",
    "src/**/*.scss"
  ],
  "scripts": {
    "compile": "tsc",
    "lint": "tslint --project tsconfig.json --type-check -c tslint.json && stylelint src/**/*.scss",
    "docs": "typedoc --options typedoc.json src",
    "pretest": "npm run compile",
    "test": "echo 'no tests'",
    "posttest": "npm run lint",
    "prebuild": "npm run test",
    "build": "webpack",
    "watch": "webpack --watch",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/igvwrapper.tar.gz --exclude \"docs\" * && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/datavisyn/igv_wrapper.git"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.1.2",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "stylelint": "^7.12.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-scss": "^1.4.4",
    "tslint": "^5.0.0",
    "typedoc": "^0.5.10",
    "url-loader": "^0.5.7",
    "typescript": "2.2.2",
    "webpack": "^2.3.3",
    "@types/handlebars": "4.0.33"
  },
  "dependencies": {
    "font-awesome": "^4.6.3"
  }
}
